<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.safenudesapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name="com.example.safenudesapp.view.Activity.LoginActivity"
            android:theme="@style/WithOut">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.safenudesapp.view.Activity.ChatActivity"
            android:theme="@style/WithOut" />

        <activity
            android:name="com.example.safenudesapp.view.Activity.MenuActivity"
            android:theme="@style/WithBar" />

        <activity
            android:name="com.example.safenudesapp.view.Activity.RegistrationActivity"
            android:theme="@style/WithOut" />
    </application>

</manifest>